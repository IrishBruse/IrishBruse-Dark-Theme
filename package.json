{
    "name": "dark-theme",
    "displayName": "IrishBruse's Dark Theme",
    "description": "My custom dark theme influenced by dark-plus-theme.",
    "version": "1.0.3",
    "publisher": "irishbruse",
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/dunstontc/dark-plus-syntax.git"
    },
    "watch": {
        "build": {
            "patterns": "src",
            "extensions": "js",
            "ignore": [
                "src/tokenColors/gen_*.js"
            ]
        }
    },
    "scripts": {
        "prebuild": "node codegen.js",
        "build": "node index.js",
        "publish": "vsce publish",
        "watch": "npm-watch"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Themes"
    ],
    "configurationDefaults": {
        "[csharp]": {
            "editor.semanticHighlighting.enabled": true
        }
    },
    "contributes": {
        "themes": [
            {
                "label": "IrishBruse's Dark Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/dark-theme.json"
            }
        ]
    },
    "devDependencies": {
        "npm-watch": "^0.11.0",
        "write-json-file": "^2.2.0"
    },
    "galleryBanner": {
        "color": "#8a76cc",
        "theme": "dark"
    },
    "icon": "assets/128.png"
}
