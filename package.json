{
    "name": "dark-theme",
    "displayName": "IrishBruse's Dark Theme",
    "description": "My custom dark theme influenced by dark-plus-theme.",
    "version": "1.5.0",
    "author": "IrishBruse",
    "publisher": "IrishBruse",
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/IrishBruse/IrishBruse-Dark-Theme"
    },
    "scripts": {
        "dev": "nodemon --watch ./src/ --watch ./utils/ ./utils/generate.js",
        "parser": "node ./utils/parser.js"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Themes",
        "Other"
    ],
    "keywords": [
        "VSCode",
        "Themes",
        "dark",
        "C#",
        "dotnet",
        "theme",
        "color-theme"
    ],
    "contributes": {
        "themes": [
            {
                "label": "IrishBruse's Dark Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/dark-theme.json"
            }
        ],
        "semanticTokenScopes": [
            {
                "language": "Java",
                "scopes": {
                    "keyword": [
                        "keyword.control.new.java"
                    ]
                }
            }
        ]
    },
    "devDependencies": {
        "nodemon": "^2.0.16"
    },
    "galleryBanner": {
        "color": "#8a76cc",
        "theme": "dark"
    },
    "icon": "assets/128.png"
}
